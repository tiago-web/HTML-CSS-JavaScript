function HouseKeeper(name, age, yearsOfExperience, languages){
    this.name = name
    this.age = age
    this.yearsOfExperience = yearsOfExperience
    this.languages = languages
    this.clean = function() {
        alert(this.name + ": Cleaning in progress...");
    }
}

var houseKeeper1 = new HouseKeeper("Maria", 26, 4, ["English", "Portuguese"])
houseKeeper1.clean();
